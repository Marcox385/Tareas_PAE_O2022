{{! IS727272 - Cordero Hernández, Marco Ricardo }}
<div class="container">
    <div class="row">
        {{#each news}}
        <div class="col-12-xs col-12-sm col-6-md col-3-lg card">
            <img src="{{this.urlToImage}}">
            <p>{{this.title}}</p>
            <p class="seymour">Ver más</p>

            <dialog>
                <p class="newsTitle">{{this.title}}</p><br>
                <p class="newsDesc">{{this.description}}</p>
                <hr>
                <p>{{this.content}}</p>
                <img src="{{this.urlToImage}}">
            </dialog>
        </div>
        {{/each}}
    </div>
</div>

<script src="/code/modal.js"></script>